{
  "name": "ntu-library-study-space",
  "version": "1.0.0",
  "description": "National Taiwan University library study space map & register. Get your seat in the elegant way.",
  "repository": "https://github.com/LucienLee/NTU-Library-Study-Space",
  "author": "Lucien Lee <lkiral7903@gmail.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "start": "npm run dev:client",
    "dev": "npm run dev:client & npm run dev:server",
    "build": "npm run build:client && npm run build:server",
    "dev:client": "node build/dev-server.js",
    "build:client": "node build/build.js",
    "dev:server": "cd server && nodemon server.js --exec 'standard \"**/*.js\" --verbose | snazzy && babel-node'",
    "build:server": "rimraf dist-server && babel --no-babelrc server --out-dir dist-server --source-maps --plugins=transform-es2015-modules-commonjs,transform-async-to-generator",
    "release": "node build/release.js",
    "release:darwin": "cross-env PLATFORM_TARGET=darwin node build/release.js",
    "release:win32": "cross-env PLATFORM_TARGET=win32 node build/release.js",
    "release:linux": "cross-env PLATFORM_TARGET=linux node build/release.js",
    "release:win32-ia32": "cross-env PLATFORM_TARGET=win32 PLATFORM_ARCH=ia32 node build/release.js",
    "unit": "karma start test/unit/karma.conf.js --single-run",
    "e2e": "node test/e2e/runner.js",
    "test": "npm run unit && npm run e2e",
    "lint": "eslint --ext .js,.vue src test/unit/specs test/e2e/specs",
    "lint:server": "standard \"server/**/*.js\" --verbose | snazzy"
  },
  "standard": {
    "ignore": [
      "**/docs/"
    ]
  },
  "dependencies": {
    "apollo-client": "^0.4.22",
    "body-parser": "^1.15.2",
    "chalk": "^1.1.3",
    "cors": "^2.8.1",
    "d3": "^4.3.0",
    "graphql": "^0.8.2",
    "graphql-server-express": "^0.4.3",
    "graphql-tools": "^0.8.4",
    "gsap": "^1.19.0",
    "lodash": "^4.16.4",
    "moment": "^2.17.1",
    "mongodb": "^2.2.11",
    "node-fetch": "^1.6.3",
    "normalize.css": "^5.0.0",
    "svg-injector": "^1.1.3",
    "vue": "^2.0.1",
    "whatwg-fetch": "^1.0.0",
    "vue-popup": "^0.2.14",
    "vuex": "^2.1.1",
    "winston": "^2.3.0"
  },
  "devDependencies": {
    "autoprefixer": "^6.6.1",
    "babel-cli": "^6.18.0",
    "babel-core": "^6.21.0",
